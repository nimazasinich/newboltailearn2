{
  "$schema": "https://raw.githubusercontent.com/modelcontextprotocol/servers/main/mcp.json",
  "mcpServers": {
    "complete-mcp-server": {
      "command": "node",
      "args": ["./complete-mcp-server.mjs"],
      "env": {
        "MEMORY_PATH": "./cursor-memory",
        "OUTPUT_PATH": "./cursor-outputs"
      },
      "disabled": false,
      "autoApprove": [
        "read_file",
        "list_tools",
        "call_tool"
      ]
    }
  }
}